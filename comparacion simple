void main()
{
	int N1,N2,N3;
	cout<<"Ingrese primer valor: ";
	cin >> N1;
	cout<<"ingrese segundo valor: ";
	cin >> N2;
	cout<<"ingrese tercer valor: ";
	cin >> N3;
	if(N1>N2)
		if (N1>N3)
			cout<<"el mayor es: "<<N1;
		else
			cout<<"el mayor es: "<<N3;
	else
		if (N2>N3)
			cout<<"el mayor es: "<<N2;
		else
			cout<<"el mayor es: "<<N3;
	getch();
}





